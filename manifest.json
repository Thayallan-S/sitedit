{
    "manifest_version": 2,
    "name": "Sitedit",
    "version": "0.1",
    "browser_action": {
        "default_popup": "static/popup.html",
        "default_icon": "img/logo.png"
    },
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery-3.4.1.min.js",
                "src/content.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}